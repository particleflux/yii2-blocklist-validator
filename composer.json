{
    "name": "particleflux/yii2-blocklist-validator",
    "description": "Validate attributes against a blocklist",
    "type": "library",
    "require": {
        "yiisoft/yii2": "^2.0",
        "php": "^8.1.0 || ^8.2.0"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "dev-develop",
        "phpmd/phpmd": "^2.14",
        "phpro/grumphp": "^2.1",
        "phpstan/phpstan": "^1.2",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Linke",
            "homepage": "https://particleflux.codes",
            "email": "particleflux@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    },
    "autoload": {
        "psr-4": {
            "particleflux\\Yii2Validators\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "particleflux\\Yii2Validators\\tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
